{
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "proseWrap": "preserve",
    "quoteProps": "consistent",
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "^(react/(.*)$)|^(react$)",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/",
        "^[../]",
        "^[./]"
    ],
    "importOrderParserPlugins": ["typescript", "jsx"]
}
